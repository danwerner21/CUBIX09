F75A                  (    cubix_ide.asm):00086         XTIDE_PROBE:
                      (    cubix_ide.asm):00087         ;
                      (    cubix_ide.asm):00088         ; BELOW TESTS FOR EXISTENCE OF AN IDE CONTROLLER ON THE
                      (    cubix_ide.asm):00089         ; PPIDE INTERFACE.  WE WRITE A VALUE OF ZERO FIRST SO THAT
                      (    cubix_ide.asm):00090         ; THE PPI BUS HOLD WILL RETURN A VALUE OF ZERO IF THERE IS
                      (    cubix_ide.asm):00091         ; NOTHING CONNECTED TO PPI PORT A.  THEN WE READ THE STATUS
                      (    cubix_ide.asm):00092         ; REGISTER.  IF AN IDE CONTROLLER IS THERE, IT SHOULD ALWAYS
                      (    cubix_ide.asm):00093         ; RETURN SOMETHING OTHER THAN ZERO.  IF AN IDE CONTROLLER IS
                      (    cubix_ide.asm):00094         ; THERE, THEN THE VALUE WRITTEN TO PPI PORT A IS IGNORED
                      (    cubix_ide.asm):00095         ; BECAUSE THE WRITE SIGNAL IS NEVER PULSED.
                      (    cubix_ide.asm):00096         
                      (    cubix_ide.asm):00097         ; CHECK SIGNATURE
                      (    cubix_ide.asm):00098         
                      (    cubix_ide.asm):00099                 LDX     #$0000
                      (    cubix_ide.asm):00100         ;       SOMETIMES THE CF-XTIDE WILL ONLY READ 80, THIS CAN BE RESET BY WRITING ZEROS UNTIL VALUES ARE PROPERLY READ
                      (    cubix_ide.asm):00101         !
                      (    cubix_ide.asm):00102                 LDB     XTIDE_DATA_LO
                      (    cubix_ide.asm):00103                 CMPB    #$80
                      (    cubix_ide.asm):00104                 BNE     >
                      (    cubix_ide.asm):00105                 LDB     #$00
                      (    cubix_ide.asm):00106                 STB     XTIDE_DATA_LO
                      (    cubix_ide.asm):00107                 NOP
                      (    cubix_ide.asm):00108                 STB     XTIDE_DATA_HI
                      (    cubix_ide.asm):00109                 NOP
                      (    cubix_ide.asm):00110                 STB     XTIDE_LBALOW
                      (    cubix_ide.asm):00111                 NOP
                      (    cubix_ide.asm):00112                 STB     XTIDE_LBAMID
                      (    cubix_ide.asm):00113                 NOP
                      (    cubix_ide.asm):00114                 STB     XTIDE_LBAHI
                      (    cubix_ide.asm):00115                 NOP
                      (    cubix_ide.asm):00116                 STB     XTIDE_DEVICE
                      (    cubix_ide.asm):00117                 NOP
                      (    cubix_ide.asm):00118                 STB     XTIDE_COMMAND
                      (    cubix_ide.asm):00119                 NOP
                      (    cubix_ide.asm):00120                 STB     XTIDE_STATUS
                      (    cubix_ide.asm):00121                 NOP
                      (    cubix_ide.asm):00122                 INX
                      (    cubix_ide.asm):00123                 CPX     #$0300
                      (    cubix_ide.asm):00124                 BNE     <
                      (    cubix_ide.asm):00125                 BRA     XTIDE_PROBE_FAIL                  ; TIMED OUT
                      (    cubix_ide.asm):00126         !
                      (    cubix_ide.asm):00127                 LDB     XTIDE_SEC_CNT
                      (    cubix_ide.asm):00128                 CMPB    #$01
                      (    cubix_ide.asm):00129                 BNE     XTIDE_PROBE_FAIL                  ; IF NOT '01' THEN REPORT NO IDE PRESENT
                      (    cubix_ide.asm):00130                 CLC
                      (    cubix_ide.asm):00131                 JMP     XTIDE_PROBE_SUCCESS
